//Exemplo de como configurar seu appsettings
{
    "defaultApp": "MyApp",
    "apps":
    {
        "MyApp": 
        {
            "name": "MyApp",
            "defaultType": "api",
            "defaultRoute": "doc/index",
            "defaultPageExtension": "html",
            "template": "carserv-1.0.0",
            "connectionStrings":
            {
                "MyDbConfig": {
                    "driver": "pgsql",
                    "host": "localhost",
                    "port": 5432,
                    "database": "db",
                    "username": "MyUserDB",
                    "password":"1234", 
                    "charset": "utf8",
                    "collation": "utf8_unicode_ci",
                    "prefix":""
                }
            }
        },
        "MyApp2":
        {
            "defaultType": "api",
            "defaultRoute": "user/authenticate"
        }
    }
}